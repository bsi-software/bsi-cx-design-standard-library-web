{% apply spaceless %}
    {% set videoElementId = elementId ?: 'video-C4cbKm' %}
    {% set videoElementPartId = elementPartId ?: 'video-part-loPxhG' %}
    {% set videoURL = (videoURL ?: properties.videoURL) ?: 'https://www.youtube.com/embed/Ekc_4btc-iM' %}
    {% set videoAltWarning = altWarning ?: properties.figureAltWarning ?: 'show-alt-warning' %}

    <div data-bsi-element="{{ videoElementId }}" class="video {{ 'bsi-element-' ~ videoElementId }} mb-3 {{ videoAltWarning }}">
        <div class="responsive-video" data-bsi-element-part="{{ videoElementPartId }}" data-bsi-video-data-link="{{ videoURL }}">
            <iframe width="1120" height="630" src="{{ videoURL }}" allow="autoplay; encrypted-media" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
        </div>
    </div>
{% endapply %}