{% macro render(elementId = null, titlePartId = null, text = null, color = null, style = null, weight = null) %}{% apply spaceless %}
    {% deprecated 'The usage of "element.render(...)" is deprecated, use "include ... with {...}" instead. For detailed information, visit: https://github.com/bsi-software/bsi-cx-design-master-template-web/blob/main/MIGRATION_GUIDE.md"' %}
    {% include './template.twig' %}
{% endapply %}{% endmacro %}

{% apply spaceless %}
    {% set titleH6ElementId = elementId ?: 'title-h6-f9a2d422' %}
    {% set titleH6PartId = titlePartId ?: 'title-h6-part-2ef79d06' %}
    {% set titleH6Text = (text ?: properties.titleH6Text) ?: bsi_cx_lorem(3) %}
    {% set titleH6Color = (color ?: properties.titleH6Color) ?: 'bsi-text-primary-color' %}
    {% set titleH6Style = (style ?: properties.titleH6Style) ?: 'bsi-header-style-6' %}
    {% set titleH6Weight = (weight ?: properties.titleH6Weight) ?: 'fw-normal' %}

    <h6 data-bsi-element="{{ titleH6ElementId }}" data-bsi-element-part="{{ titleH6PartId }}" class="{{ 'bsi-element-' ~ titleH6ElementId }} bsi-title-element {{ titleH6Color }} {{ titleH6Style }} {{ titleH6Weight }}">{{ titleH6Text }}</h6>
{% endapply %}